<%= form_for location do |f| %>
<%= f.label :name, id: 'label'  %>
<%= f.text_field :name, id: 'label'  %>
<br />
<%= f.label :street_number, id: 'label'  %>
<%= f.text_field :street_number, id: 'label'  %>
<br />
<%= f.label :roadid, id: 'label'  %>
<%= f.text_field :road, id: 'label'  %>
<br />
<%= f.label :city, id: 'label'  %>
<%= f.text_field :city, id: 'label'  %>
<br />
<%= f.label :state, id: 'label'  %>
<%= f.text_field :state, id: 'label'  %>
<br />
<%= f.hidden_field :longitude %>
<%= f.hidden_field :latitude %>
<%= f.hidden_field :address %>
<%= f.hidden_field :formatted_address %>
<%= f.hidden_field :country %>
<br />
<%= f.submit %>
<% end %>
