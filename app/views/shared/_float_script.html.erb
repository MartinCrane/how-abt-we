<script>

var multiply = 0;
function twist() {
  var PadWidth = Math.sin(multiply) *.001;
  var PadHeight = Math.sin(multiply) *.002;
  var PadSkew = Math.sin(multiply) * .002;
  multiply = multiply + .001;
  $( ".float" ).css('-webkit-transform','matrix3d(1,'+PadSkew+',0,'+PadSkew+','+PadHeight+',1,'+PadHeight+',0,0,0,1,0,5,0,0,1)');
}
setInterval(function(){
twist();
}, 1);
var colorR = 0
var colorG = 0
var colorB = 0

function color_twist() {
  var Red = Math.abs(parseInt((Math.sin(colorR) * 300)));
  var Green = Math.abs(parseInt((Math.sin(colorG**2) * 30)));
  var Blue = Math.abs(parseInt((Math.sin(colorB*2) * 30)));
  colorR = colorR + .001;
  colorG = colorG + .002;
  colorB = colorB + .004;

  $( ".color_change" ).css('color','rgb('+Red+','+Green+','+Blue+')');
  // $( ".color_change" ).css('background-color','rgb('+Blue+','+Green+','+Red+')');

}

setInterval(function(){
color_twist();
}, 1);

</script>
